{
  "name": "voting-client",
  "version": "0.0.1",
  "description": "In an effort to always be learning, I am seeking to improve my skills with JavaScript (including ES6), React, and Redux. I am also learning volumes about how to test my JavaScript using Mocha and Chai.",
  "main": "dist/index.html",
  "scripts": {
    "start": "webpack-dev-server",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js 'test/**/*.@(js|jsx)'",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewtpoe/voting-client.git"
  },
  "keywords": [
    "learning"
  ],
  "author": "Andrew T. Poe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewtpoe/voting-client/issues"
  },
  "homepage": "https://github.com/andrewtpoe/voting-client#readme",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "jsdom": "^7.2.2",
    "mocha": "^2.3.4",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "history": "^1.17.0",
    "immutable": "^3.7.6",
    "react": "^0.14.5",
    "react-addons-pure-render-mixin": "^0.14.5",
    "react-dom": "^0.14.5",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "socket.io-client": "^1.4.0"
  }
